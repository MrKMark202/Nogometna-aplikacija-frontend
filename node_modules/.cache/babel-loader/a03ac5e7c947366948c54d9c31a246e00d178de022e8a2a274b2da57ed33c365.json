{"ast":null,"code":"import { Auth } from '@/components/registracija';\nimport axios from 'axios';\nexport default {\n  data: () => ({\n    auth: Auth.state,\n    userData: []\n  }),\n  mounted() {\n    this.dohvatiKorisnika();\n  },\n  methods: {\n    async dohvatiKorisnika() {\n      try {\n        const userEmail = this.auth.userEmail;\n        const response = await axios.get(`http://localhost:10000/api/user/dohvat?email=${userEmail}`);\n        if (response.status !== 200) {\n          throw new Error('Network response was not ok');\n        }\n        this.userData = response.data;\n        console.log(this.userData);\n      } catch (error) {\n        console.error('Greška prilikom dohvaćanja podataka korisnika:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["Auth","axios","data","auth","state","userData","mounted","dohvatiKorisnika","methods","userEmail","response","get","status","Error","console","log","error"],"sources":["src/views/userSettings.vue"],"sourcesContent":["<template>\r\n    <div class=\"obrub\">\r\n        <div class=\"naslov\">\r\n            <h1>Korisničke postavke</h1>\r\n        </div>\r\n        <v-img :src=\"auth.userProfilePicture\" class=\"profilna\"></v-img>\r\n\r\n        <h3 class=\"tekst\">Ime</h3>\r\n        <h3 class=\"podaci\">{{ userData.ime }}</h3>\r\n\r\n        <h3 class=\"tekst\">Prezime</h3>\r\n        <h3 class=\"podaci\">{{ userData.prezime }}</h3>\r\n\r\n        <h3 class=\"tekst\">Datum rođenja</h3>\r\n        <h3 class=\"podaci\">{{ userData.datumRodenja}}</h3>\r\n\r\n        <h3 class=\"tekst\">Email korisnika</h3>\r\n        <h3 class=\"podaci\">{{ userData.email }}</h3>\r\n    </div>\r\n</template>\r\n<script>\r\n    import { Auth } from '@/components/registracija'\r\n    import axios from 'axios';\r\n\r\n    export default {\r\n        data: () => ({\r\n            auth: Auth.state,\r\n            userData: [],\r\n        }),\r\n\r\n        mounted() {\r\n            this.dohvatiKorisnika();\r\n        },\r\n\r\n        methods: {\r\n           async dohvatiKorisnika() {\r\n                try {\r\n                const userEmail = this.auth.userEmail;\r\n                const response = await axios.get(`http://localhost:10000/api/user/dohvat?email=${userEmail}`);\r\n                if (response.status !== 200) {\r\n                    throw new Error('Network response was not ok');\r\n                } \r\n                this.userData = response.data\r\n                console.log(this.userData)\r\n                } catch (error) {\r\n                    console.error('Greška prilikom dohvaćanja podataka korisnika:', error);\r\n                }\r\n            },\r\n        }\r\n    }\r\n\r\n\r\n\r\n</script>\r\n<style scoped>\r\n    .obrub\r\n    {\r\n        border: 2px solid white;\r\n        background-color: white;\r\n        padding: 50px;\r\n        margin-left: 20%;\r\n        margin-right: 20%;\r\n        margin-top: 100px; \r\n    }\r\n\r\n    .profilna\r\n    {\r\n        width: 200px;\r\n        height: 200px;\r\n        border-radius: 50%;\r\n        background-color: Green; \r\n        margin-right: 10px;\r\n    }\r\n\r\n    .tekst\r\n    {\r\n        color: black;\r\n        font-size: 50px;\r\n        margin-top: 10px;\r\n    }\r\n\r\n    .podaci\r\n    {\r\n        color: black;\r\n        font-size: 30px;\r\n        margin-top: 10px;\r\n        border: 2px green double;\r\n    }\r\n</style>"],"mappings":"AAqBA,SAAAA,IAAA;AACA,OAAAC,KAAA;AAEA;EACAC,IAAA,EAAAA,CAAA;IACAC,IAAA,EAAAH,IAAA,CAAAI,KAAA;IACAC,QAAA;EACA;EAEAC,QAAA;IACA,KAAAC,gBAAA;EACA;EAEAC,OAAA;IACA,MAAAD,iBAAA;MACA;QACA,MAAAE,SAAA,QAAAN,IAAA,CAAAM,SAAA;QACA,MAAAC,QAAA,SAAAT,KAAA,CAAAU,GAAA,iDAAAF,SAAA;QACA,IAAAC,QAAA,CAAAE,MAAA;UACA,UAAAC,KAAA;QACA;QACA,KAAAR,QAAA,GAAAK,QAAA,CAAAR,IAAA;QACAY,OAAA,CAAAC,GAAA,MAAAV,QAAA;MACA,SAAAW,KAAA;QACAF,OAAA,CAAAE,KAAA,mDAAAA,KAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}